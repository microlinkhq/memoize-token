{
  "name": "memoize-token",
  "description": "A memoize library for refreshing and cache token values.",
  "homepage": "https://nicedoc.io/microlinkhq/memoize-token",
  "version": "1.1.6",
  "main": "index.js",
  "author": {
    "email": "hello@microlink.io",
    "name": "microlink.io",
    "url": "https://microlink.io"
  },
  "contributors": [
    {
      "name": "Kiko Beats",
      "email": "josefrancisco.verdu@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/microlinkhq/memoize-token.git"
  },
  "bugs": {
    "url": "https://github.com/microlinkhq/memoize-token/issues"
  },
  "keywords": [
    "api",
    "cache",
    "call",
    "hit",
    "memo",
    "memoization",
    "memoize",
    "perfomance",
    "refresh",
    "token"
  ],
  "dependencies": {
    "debug": "~4.3.1"
  },
  "devDependencies": {
    "@commitlint/cli": "latest",
    "@commitlint/config-conventional": "latest",
    "ava": "latest",
    "c8": "latest",
    "ci-publish": "latest",
    "conventional-github-releaser": "latest",
    "delay": "latest",
    "finepack": "latest",
    "git-authors-cli": "latest",
    "nano-staged": "latest",
    "npm-check-updates": "latest",
    "prettier-standard": "latest",
    "simple-git-hooks": "latest",
    "standard": "latest",
    "standard-markdown": "latest",
    "standard-version": "latest"
  },
  "engines": {
    "node": ">= 8"
  },
  "files": [
    "index.js"
  ],
  "scripts": {
    "clean": "rm -rf node_modules",
    "commitmsg": "commitlint -E GIT_PARAMS",
    "contributors": "(git-authors-cli && finepack && git add package.json && git commit -m 'build: contributors' --no-verify) || true",
    "lint": "standard-markdown README.md && standard",
    "postrelease": "npm run release:tags && npm run release:github && (ci-publish || npm publish --access=public)",
    "prerelease": "npm run update:check && npm run contributors",
    "pretest": "npm run lint",
    "pretty": "prettier-standard index.js {core,test,bin,scripts}/**/*.js --single-quote --print-width 100",
    "release": "standard-version -a",
    "release:github": "conventional-github-releaser -p angular",
    "release:tags": "git push --follow-tags origin HEAD:master",
    "test": "c8 ava",
    "update": "ncu -u",
    "update:check": "ncu -- --error-level 2"
  },
  "license": "MIT",
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "nano-staged": {
    "*.js": [
      "git add",
      "prettier-standard"
    ],
    "*.md": [
      "git add",
      "standard-markdown"
    ],
    "package.json": [
      "finepack",
      "git add"
    ]
  },
  "simple-git-hooks": {
    "commit-msg": "npx commitlint --edit",
    "pre-commit": "npx nano-staged"
  }
}
